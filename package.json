{
  "name": "@xsoulspace/vue_flutter_tailwind",
  "description": "Vue3 styled like Flutter with Tailwind CSS",
  "version": "0.7.0",
  "private": false,
  "author": {
    "name": "Anton Malofeev",
    "email": "anton@xsoulspace.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xsoulspace/vue_flutter_tailwind.git"
  },
  "bugs": {
    "url": "https://github.com/xsoulspace/vue_flutter_tailwind/issues"
  },
  "homepage": "https://github.com/xsoulspace/vue_flutter_tailwind#readme",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
