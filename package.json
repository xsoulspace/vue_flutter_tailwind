    "@vue/test-utils": "^2.0.0-0",
    "eslint": "^7.20.0",
    "autoprefixer": "^9",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
    "sass-loader": "^10.1.1",
    "typescript": "^4.1.5",
    "vue-jest": "^5.0.0-0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
